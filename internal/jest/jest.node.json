{
  "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
  },
  "collectCoverage": true,
  "coverageDirectory": "<rootDir>/coverage/node/coverage-final.json",
  "coveragePathIgnorePatterns": [
    "<rootDir>/(internal|config|flow-typed|docs|boldrCMS|.happypack|bin|.idea|shared|client|public|db)/",
    "/node_modules/",
    "/boldrSSR/"
  ],
  "transform": {
    "^.+\\.js$": "<rootDir>/internal/jest/transform.js"
  },
  "testRegex": ".*.spec\\.js",
  "setupTestFrameworkScriptFile": "<rootDir>/internal/jest/nodeSetup.js",
  "testPathIgnorePatterns": [
    "<rootDir>/(internal|config|docs|shared|client|boldrCMS|.happypack|bin|.idea|public|db)/",
    "__snapshots__"
  ],
  "testEnvironment": "node",
  "transformIgnorePatterns": [
    "/node_modules/",
    "/.happypack/",
    "/boldrCMS/",
    "/db/"
  ],
  "coverageReporters": [
    "json"
  ]
}
